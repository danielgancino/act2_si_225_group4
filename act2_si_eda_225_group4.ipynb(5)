      "metadata": {
        "id": "c-Fsy7AVXKXL"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "months_labels = ['jan', 'feb', 'mar', 'apr', 'may', 'jun', 'jul', 'aug', 'sep', 'oct', 'nov', 'dic']\n",
        "df['review_month'].value_counts().reindex(range(1,13), fill_value=0).plot(kind='bar', title='Reviews per Month', xlabel='Month', ylabel='Amount')\n",
        "plt.xticks(ticks=range(12), labels=months_labels, rotation=0)\n",
        "plt.show()"
      ],
